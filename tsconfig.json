{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./static/js",
    "rootDir": "./static/ts",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "static/ts/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}